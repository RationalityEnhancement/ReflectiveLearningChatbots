[
  {
    "qId" : "startOnboarding",
    "qType" : "dummy",
    "replyMessages": {
      "English" : ["Let us begin with the first questions already!"],
      "Deutsch" : ["Fangen wir schon mit den ersten Fragen an!"]
    },
    "nextQuestion" : "Onboarding.survey"
  },
  {
    "qId" : "initiateContact",
    "qType" : "dummy",
    "cNextQuestions" : [
      {
        "if" : "(${STAGE_DAY} > $N{3}) AND (${warningGiven} != $B{true})",
        "then" : "Onboarding.wantContinue"
      },
      {
        "if" : "(${STAGE_DAY} > $N{3}) AND (${warningGiven} == $B{true})",
        "then" : "Onboarding.endExperiment",
        "else" : "Onboarding.survey"
      }
    ],
    "selectQFirst" : true,
    "cNextActions" : {
      "if" : "(${STAGE_DAY} > $N{3}) AND (${warningGiven} != $B{true})",
      "then" : [
        {
          "aType" : "setBooleanVar",
          "args" : ["warningGiven", "$B{true}"]
        }
      ]
    },
    "replyMessages" : {
      "English" : ["Good morning, ${FIRST_NAME}!"],
      "Deutsch" : ["Guten Morgen, ${FIRST_NAME}!"]
    }
  },
  {
    "qId" : "wantContinue",
    "qType" : "singleChoice",
    "text" : {
      "English" : "Would you like to continue with the experiment or would you like to end it here and stop receiving messages?",
      "Deutsch" : "Übersetzung nicht verfügbar?"
    },
    "options" : {
      "English" : ["Continue", "End Experiment"],
      "Deutsch" : ["Continue", "End Experiment"]
    },
    "cNextQuestions" : [
      {
        "if" : "${CURRENT_ANSWER} HAS_CHOICE_IDX $N*{1}",
        "then" : "Onboarding.endExperiment",
        "else" : "Onboarding.survey"
      }
    ],
    "cReplyMessages" : [
      {
        "if" : "${CURRENT_ANSWER} HAS_CHOICE_IDX $N*{0}",
        "then" : {
          "English" : ["Great to hear from you again and that you want to continue the experiment!", "Follow the instructions to continue."],
          "Deutsch" : ["Übersetzung nicht verfügbar."]
        }
      }
    ]
  },
  {
    "qId" : "endExperiment",
    "qType" : "dummy",
    "nextActions" : [
      {
        "aType" : "endExperiment"
      }
    ],
    "replyMessages" : {
      "English" : ["The experiment has been ended."],
      "Deutsch" : ["Übersetzung nicht verfügbar."]
    }
  },
  {
    "qId" : "survey",
    "qType" : "qualtrics",
    "text" : {
      "English" : "The first thing we need from you is some basic information. Please take the time to fill out the following survey thoughtfully and thoroughly.",
      "Deutsch" : "Wir brauchen zuerst von Ihnen einige Basisinformationen. Bitte nehmen Sie sich die Zeit, die folgende Umfrage bedacht und vollständig auszufüllen."
    },
    "qualtricsLink" : "https://cuboulder.qualtrics.com/jfe/form/SV_dgTBD73gMupCsJM",
    "qualtricsFields" : [
      {
        "field" : "uniqueId",
        "value" : "${UNIQUE_ID}"
      },
      {
        "field" : "condition",
        "value" : "${CONDITION}"
      },
      {
        "field" : "stage",
        "value": "Onboarding"
      }
    ],
    "nextActions" : [
      {
        "aType" : "setBooleanVar",
        "args" : ["onboardingComplete", "$B{true}"]
      },
      {
        "aType" : "setBooleanVar",
        "args" : ["warningGiven", "$B{false}"]
      }
    ],
    "inputPrompt" : {
      "English" : "Enter the survey code that you receive at the end of the survey to continue.",
      "Deutsch" : "Geben Sie den Umfrage-Code ein, den Sie am Ende der Umfrage erhalten, um fortzufahren."
    },
    "continueStrings" : ["VARIETY"],
    "replyMessages" : {
      "English" : ["Thank you for that!", "That’s all for today. On the next weekday, we will start our daily routine of setting goals in the morning and then reflecting about them in the evening, which will last three days.", "See you later!"],
      "Deutsch" : ["Übersetzung nicht verfügbar", "Bis später!"]
    },
    "reminder" : {
      "freqMins" : 30,
      "numRepeats" : 4
    }
  },
  {
    "qId" : "checkComplete",
    "qType" : "dummy",
    "cNextActions" : [
      {
        "if": "${onboardingComplete} == $B{true}",
        "then" : [{
          "aType" : "startStage",
          "args" : ["Intervention"]
        }]
      }
    ]
  }
]