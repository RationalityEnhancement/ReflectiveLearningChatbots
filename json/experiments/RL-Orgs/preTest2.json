[
  {
    "qId" : "initiateContact",
    "qType" : "dummy",
    "cNextQuestions" : [
      {
        "if" : "(${STAGE_DAY} > $N{3}) AND (${warningGiven} != $B{true})",
        "then" : "Pre-Test-2.wantContinue"
      },
      {
        "if" : "(${STAGE_DAY} > $N{3}) AND (${warningGiven} == $B{true})",
        "then" : "Pre-Test-2.endExperiment",
        "else" : "Pre-Test-2.survey"
      }
    ],
    "selectQFirst" : true,
    "cNextActions" : {
      "if" : "(${STAGE_DAY} > $N{3}) AND (${warningGiven} != $B{true})",
      "then" : [
        {
          "aType" : "setBooleanVar",
          "args" : ["warningGiven", "$B{true}"]
        }
      ]
    },
    "replyMessages" : {
      "English" : ["Good morning, ${FIRST_NAME}!"],
      "Deutsch" : ["Guten Morgen, ${FIRST_NAME}!"]
    }
  },
  {
    "qId" : "wantContinue",
    "qType" : "singleChoice",
    "text" : {
      "English" : "Would you like to continue with the experiment or end it now? If you choose to end it, you will stop receiving messages.",
      "Deutsch" : "Möchten Sie mit dem Experiment fortfahren oder es jetzt beenden? Wenn Sie sich für das Beenden entscheiden, werden Sie keine Nachrichten mehr erhalten."
    },
    "options" : {
      "English" : ["Continue", "End Experiment"],
      "Deutsch" : ["Fortfahren", "Experiment Beenden"]
    },
    "cNextQuestions" : [
      {
        "if" : "${CURRENT_ANSWER} HAS_CHOICE_IDX $N*{1}",
        "then" : "Pre-Test-2.endExperiment",
        "else" : "Pre-Test-2.survey"
      }
    ],
    "cReplyMessages" : [
      {
        "if" : "${CURRENT_ANSWER} HAS_CHOICE_IDX $N*{0}",
        "then" : {
          "English" : ["Great to hear from you. I see you want to continue the experiment!", "Please follow the instructions to continue."],
          "Deutsch" : ["Schön, dass Sie das Experiment fortsetzen möchten!", "Bitte folgen Sie den Anweisungen um fortzufahren."]
        }
      }
    ]
  },
  {
    "qId" : "endExperiment",
    "qType" : "dummy",
    "nextActions" : [
      {
        "aType" : "endExperiment"
      }
    ],
    "replyMessages" : {
      "English" : ["The experiment has ended."],
      "Deutsch" : ["Das Experiment ist beendet."]
    }
  },
  {
    "qId" : "survey",
    "qType" : "qualtrics",
    "text" : {
      "English" : "Please take the time to fill out the following survey thoughtfully and thoroughly.",
      "Deutsch" : "Bitte nehmen Sie sich die Zeit, die nachfolgende Fragebogen sorgfältig und vollständig auszufüllen."
    },
    "qualtricsLink" : "https://cuboulder.qualtrics.com/jfe/form/SV_8ffND85CsuavdkO",
    "qualtricsFields" : [
      {
        "field" : "uniqueId",
        "value" : "${UNIQUE_ID}"
      },
      {
        "field" : "condition",
        "value" : "${CONDITION}"
      },
      {
        "field" : "stage",
        "value": "Pre-Test-2"
      }
    ],
    "reminder" : {
      "freqMins" : 30,
      "numRepeats" : 4
    },
    "inputPrompt" : {
      "English" : "Enter the survey code displayed at the end of the survey to continue. (ABCDE, 12345)",
      "Deutsch" : "Geben Sie den am Ende des Fragebogens angezeigten Code ein, um fortzufahren. (ABCDE, 12345)"
    },
    "continueStrings" : ["ABCDE", "12345"],
    "nextActions" : [
      {
        "aType" : "setBooleanVar",
        "args" : ["preTest2Complete", "$B{true}"]
      },
      {
        "aType" : "setBooleanVar",
        "args" : ["warningGiven", "$B{false}"]
      }
    ],
    "replyMessages" : {
      "English" : ["Thank you!", "That’s all for today. On the next weekday, we will start another daily routine of setting goals in the morning and then reflecting on them in the evening. This will last for five days again.", "See you later!"],
      "Deutsch" : ["Vielen Dank!", "Das war's für heute. Wir werden am nächsten Werktag mit noch einer täglichen Routine beginnen, bei der Sie sich morgens Ziele setzen und sie abends reflektieren. Das Ganze machen wir fünf Tage lang wieder.", "Bis später!"]
    }
  },
  {
    "qId" : "checkComplete",
    "qType" : "dummy",
    "cNextActions" : [
      {
        "if": "${preTest2Complete} == $B{true}",
        "then" : [
          {
            "aType" : "clearVars",
            "args" : ["remindWorkIntention", "workIntention", "relIntention", "remindRelIntention",
              "taskGoals", "relGoals", "taskGoalsSetToday", "relGoalsSetToday","lastReportedTaskProgress",
            "taskProgressReportedOnce", "lastReportedRelProgress", "relProgressReportedOnce", "dontAskInfoAgain",
            "dontAskExampleAgain", "dontAskRelInfoAgain", "dontAskRelExampleAgain","relGoalsSetBefore",
            "firstRelGoalDay", "firstWorkGoalDay", "workGoalsSetBefore", "reflectionStartedToday", "warningGiven"]
          },
          {
            "aType" : "startStage",
            "args" : ["Intervention"]
          }
        ]
      }
    ]
  }
]